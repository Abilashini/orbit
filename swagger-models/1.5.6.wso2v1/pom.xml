<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>
    <groupId>org.wso2.orbit.io.swagger</groupId>
    <artifactId>swagger-models</artifactId>
    <version>1.5.6.wso2v1</version>
    <packaging>bundle</packaging>
    <name>WSO2 Carbon Orbit - swagger-models (io.swagger)</name>
    <description>
        This bundle will export packages from swagger-models libraries of io.swagger
    </description>
    <url>http://wso2.org</url>

    <distributionManagement>
        <repository>
            <id>wso2.releases</id>
            <name>WSO2 internal Repository</name>
            <url>http://maven.wso2.org/nexus/content/repositories/releases/</url>
        </repository>

        <snapshotRepository>
            <id>wso2.snapshots</id>
            <name>Apache Snapshot Repository</name>
            <url>http://maven.wso2.org/nexus/content/repositories/snapshots/</url>
        </snapshotRepository>
    </distributionManagement>

    <dependencies>
        <dependency>
            <groupId>io.swagger</groupId>
            <artifactId>swagger-models</artifactId>
            <version>1.5.6</version>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <version>2.4.0</version>
                <extensions>true</extensions>
                <configuration>
                    <instructions>
                        <Bundle-SymbolicName>${project.artifactId}</Bundle-SymbolicName>
                        <Bundle-Name>${project.artifactId}</Bundle-Name>
                        <Export-Package>
                            io.swagger.models.auth;uses:="com.fasterxml.jackson.annotation";version="1.5.6",
                            io.swagger.models.parameters;uses:="com.fasterxml.jackson.annotation,io.swagger.models.properties,org.slf4j,io.swagger.models,io.swagger.models.refs";version="1.5.6",
                            io.swagger.models.properties;uses:="io.swagger.models,com.fasterxml.jackson.annotation,org.slf4j,io.swagger.models.refs";version="1.5.6",
                            io.swagger.models.refs;version="1.5.6.SNAPSHOT",
                            io.swagger.models;uses:="com.fasterxml.jackson.annotation,io.swagger.models.properties,javax.xml.bind.annotation,io.swagger.models.parameters,io.swagger.models.refs,io.swagger.models.auth";version="1.5.6"
                        </Export-Package>
                        <Import-Package>
                            com.fasterxml.jackson.annotation;version="[2.4,3)",
                            javax.xml.bind.annotation,org.slf4j;version="[1.6,2)"
                        </Import-Package>
                        <Private-Package>
                        </Private-Package>
                        <Include-Resource>
                            {maven-resources}
                        </Include-Resource>
                    </instructions>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <properties>
        <export.pkg.version.swagger-models>1.5.6</export.pkg.version.swagger-models>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>
</project>
